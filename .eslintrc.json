{
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "project": ["./tsconfig.json"]
  },
  "extends": [
    "@zendeskgarden",
    "@zendeskgarden/eslint-config/plugins/notice",
    "@zendeskgarden/eslint-config/plugins/typescript",
    "@zendeskgarden/eslint-config/plugins/typescript-semantics",
    "prettier",
    "prettier/@typescript-eslint"
  ],
  "rules": {
    "no-console": "off",
    "node/no-unsupported-features/node-builtins": ["error", { "version": ">=10.0.0" }]
  }
}
